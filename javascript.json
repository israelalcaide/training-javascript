"Random number between max and min": {
    "prefix": "rn",
    "body": [
      "let max = ${1:num1};",
      "let min = ${2:num2};",
      "let random = Math.floor(Math.random() * (max - min + 1) ) + min;",
      "console.log(random)"
    ],
    "description": "Random number between max and min"
  }

