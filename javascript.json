  "improved switch": {
    "prefix": "sw",
    "body": [
      "switch (${1:key}) {",
      "    case value:",
      "        console.log(\"${2:message}\");",
      "        break;",
      "    case \"value\":",
      "        console.log(\"${3:message}\");",
      "        break;",
      "",
      "    default:",
      "        console.log(\"${4:message}\");",
