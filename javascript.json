"if - else if - else": {
    "prefix": "ie",
    "body": [
      "if (${1:condition}) {",
      "  console.log("${2:message}");",
      "} else if (${3:condition}) {",
      "  console.log("${4:message}");",
      "} else {",
      "  console.log("${5:message}");",
      "}"
    ],
    "description": "if - else if - else"
  }
